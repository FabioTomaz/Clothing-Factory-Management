<Window x:Class="Trabalho_BD_IHC.Imagem"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:wpfTool="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Trabalho_BD_IHC"
        mc:Ignorable="d"
        Title="Imagem" Height="500" Width="500" Icon="draw.png">
    <Grid>    
        <wpfTool:MagnifierManager.Magnifier>
            <wpfTool:Magnifier Radius="100" ZoomFactor=".4" />
        </wpfTool:MagnifierManager.Magnifier>
        <Image Name="image"></Image>
    </Grid>
</Window>
